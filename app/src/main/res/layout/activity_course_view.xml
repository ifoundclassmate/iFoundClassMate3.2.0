<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="heyheyoheyhey.com.ifoundclassmate3.CourseViewActivity">
    <com.astuetz.PagerSlidingTabStrip
        android:id="@+id/courseViewTabs"
        android:layout_width="match_parent"
        android:layout_height="48dip"
        android:background="#ff76c4ff" />

    <android.support.v4.view.ViewPager
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/courseViewTabs"
        tools:context="heyheyoheyhey.com.ifoundclassmate3.DayActivity"
        android:id="@+id/courseViewPager" />
</RelativeLayout>
